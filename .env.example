# MinIO Server Configuration
MINIO_SERVER=localhost
MINIO_PORT=9000
MINIO_SECURE=false

# MinIO Bucket Creator Credentials
BUCKET_CREATOR_ACCESS_KEY=your_bucket_creator_access_key
BUCKET_CREATOR_SECRET_KEY=your_bucket_creator_secret_key

# MinIO Admin Credentials  
MINIO_ADMIN_ACCESS_KEY=your_admin_access_key
MINIO_ADMIN_SECRET_KEY=your_admin_secret_key

# HashiCorp Vault Configuration (REQUIRED)
# Note: All Vault environment variables are required for security
VAULT_ADDR=https://your-vault-server:8200
VAULT_ROLE_ID=your_vault_role_id_here
VAULT_SECRET_ID=your_vault_secret_id_here