{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject",
        "s3:PutObject"
      ],
      "Resource": [
        "arn:aws:s3:::concourse-pipeline-logs",
        "arn:aws:s3:::concourse-pipeline-logs/*",
        "arn:aws:s3:::concourse-pipeline-build-artifacts",
        "arn:aws:s3:::concourse-pipeline-build-artifacts/*"
      ]
    }
  ]
}

